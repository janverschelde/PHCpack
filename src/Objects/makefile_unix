	gcc -c $(PHCLib)/syscon.c
	gcc -c $(PHCLib)/solcon.c
	gcc -c $(PHCLib)/phcpack.c
	gcc -c $(PHCLib)/phc_solve.c
	gnatbind -n use_c2phc4c
	gnatlink -C use_c2phc4c syscon.o solcon.o phcpack.o phc_solve.o \
                 -o $(BIN)/phc_solve -lm

