gpunewton_qd.o: 
	$(gpp) -O2 -fPIC -c $(PHCAda)/PHCtoC/gpunewton_qd.cpp -I$(PHCLib) \
            -I $(PHCgpu)/Path/DefineTypesQD -I $(PHCgpu)/Path/Main \
