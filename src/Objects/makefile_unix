            -I$(PHCgpu)/Path/DefineTypesD -I$(PHCgpu)/Path/Poly \
            -I$(PHCgpu)/Path/Complex -I$(PHCgpu)/Path/Poly \
            -c $(PHCLib)/adepath_d.c
