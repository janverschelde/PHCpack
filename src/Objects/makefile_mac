	gnatbind -n use_c2phc4c
	gnatlink -C use_c2phc4c syscon.o solcon.o schubert.o lib_lrhom.o \
                    /usr/lib/libc++.dylib /usr/lib/libstdc++.dylib \
                 -o $(BIN)/lib_lrhom \
                 -lm --LINK=$(gpp) -Wl,-no_compact_unwind 

