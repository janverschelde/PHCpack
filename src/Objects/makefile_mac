	$(gcc) -c $(PHCLib)/lib_syscon.c
	gnatbind -n use_c2phc4c
	gnatlink -C use_c2phc4c syscon.o lib_syscon.o \
                 -o $(BIN)/lib_syscon --LINK=$(gcc) -Wl,-no_compact_unwind

