	$(gcc) -c $(PHCLib)/phcpack.c

use_c2phc.o:
	gnatmake -c $(INCLULIBS) $(GNATOPTFLAGS) use_c2phc.adb

ts_list2str:
	$(gcc) -c $(PHCLib)/lists_and_strings.c
	$(gcc) -c $(PHCLib)/ts_list2str.c
	$(gcc) lists_and_strings.o ts_list2str.o -o $(BIN)/ts_list2str

