smTCGemm:
	nvcc -ccbin=/usr/bin/gcc -O3 -o smTCGemm -arch=sm_80 \
             simpleTensorCoreGemm.cu

pi64bits:
	gcc -o pi64bits bits_of_pi.c

test_split:
	@-echo "compiling ..."
	g++ -c splitting_doubles.cpp -o splitting_doubles.o
	g++ -c test_splitting_doubles.cpp -o test_splitting_doubles.o
	@-echo "linking ..."
	g++ -o test_split test_splitting_doubles.o splitting_doubles.o

clean:
	/bin/rm -f -r smTCGemm pi64bits
	/bin/rm -f -r test_split test_splitting_doubles.o splitting_doubles.o
