test_dbl_cycsys: random_numbers.o random_series.o random_monomials.o \
                 random_polynomials.o convolution_job.o convolution_jobs.o \
                 addition_job.o addition_jobs.o job_makers.o \
                 complexconv_job.o complexconv_jobs.o \
                 complexinc_job.o complexinc_jobs.o \
                 complexadd_job.o complexadd_jobs.o \
                 job_coordinates.o write_job_counts.o write_gpu_timings.o \
                 dbl_convolutions_host.o dbl_monomials_host.o \
                 dbl_polynomials_host.o dbl_polynomials_kernels.o \
                 dbl_polynomials_testers.o cyclic_indices.o \
                 dbl_indexed_coefficients.o test_dbl_cycsys.o
	@-echo ">>> linking ..."
	$(gpp) -o test_dbl_cycsys test_dbl_cycsys.o cyclic_indices.o \
                  random_numbers.o random_series.o random_monomials.o \
                  random_polynomials.o convolution_job.o convolution_jobs.o \
                  addition_job.o addition_jobs.o job_makers.o \
                  complexconv_job.o complexconv_jobs.o \
                  complexinc_job.o complexinc_jobs.o \
                  complexadd_job.o complexadd_jobs.o \
                  job_coordinates.o write_job_counts.o write_gpu_timings.o \
                  dbl_convolutions_host.o dbl_monomials_host.o \
                  dbl_polynomials_host.o dbl_polynomials_kernels.o \
                  dbl_polynomials_testers.o dbl_indexed_coefficients.o \
               -lcuda -lcudart -L$(CUDALIB)

