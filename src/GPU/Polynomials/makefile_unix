             --define-macro gpufun=1 \
             -c dbl10_polynomials_kernels.cu

polynomials_kernels.o:
	@-echo ">>> compiling all polynomial kernels ..."
	nvcc -ccbin=$(gpp) -arch=$(smflag) -I$(MPD) -I$(CNV) \
             -c polynomials_kernels.cu

